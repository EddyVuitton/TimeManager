<MudDialog>
    <DialogContent>
        <MudTextField @bind-Value="@ListDto.Name" Variant="Variant.Text" Placeholder="Wpisz nazwę" ErrorText="@_errorText" Error="@isError" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Anuluj</MudButton>
        <MudButton Color="Color.Primary" OnClick="(async () => await Submit())">Gotowe</MudButton>
    </DialogActions>
</MudDialog>